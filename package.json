{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "prebuild:dev": "npm run clean:build",
    "build:dev": "rollup -c -w",
    "prebuild:prod": "echo 'BASE_API_URL=${BASE_API_URL}' >> .env; echo 'GOOGLE_PLACES_API_KEY=${GOOGLE_PLACES_API_KEY}' >> .env; echo 'GOOGLE_ANALYTICS_ID=${'//httpbin.org'}' >> .env; npm run clean:build;",
    "build:prod": "rollup -c",
    "clean": "run-p -l clean:build clean:deps",
    "clean:build": "rimraf dist/bundle* dist/service-worker* dist/workbox*",
    "clean:deps": "npm prune",
    "local:dev": "run-p -l build:dev serve:http",
    "local:prod": "run-p -l build:prod serve:http",
    "serve:http": "sirv dist --dev --host 0.0.0.0 --port 5000 --single"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@roxi/routify": "^2.15.1",
    "@silintl/svelte-google-places-autocomplete": "^1.1.1",
    "@silintl/ui-components": "^2.3.1",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.12.0",
    "fa-svelte": "^3.1.0",
    "jquery": "^3.5.1",
    "material-components-web": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss": "^8.2.13",
    "qs": "^6.10.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "sass": "^1.32.12",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.38.2",
    "svelte-preprocess": "^4.7.3"
  }
}
