{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "prebuild:dev": "npm run clean:build",
    "build:dev": "rollup -c -w",
    "prebuild:prod": "npm run clean:build",
    "build:prod": "rollup -c",
    "clean": "run-p -l clean:build clean:deps",
    "clean:build": "rimraf dist/bundle* dist/service-worker* dist/workbox*",
    "clean:deps": "npm prune",
    "local:dev": "run-p -l build:dev serve:http",
    "local:prod": "run-p -l build:prod serve:http",
    "serve:http": "sirv dist --dev --host 0.0.0.0 --port 5001 --single"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@roxi/routify": "^2.18.4",
    "@silintl/svelte-google-places-autocomplete": "^1.1.6",
    "@silintl/ui-components": "^3.12.0",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.28.0",
    "fa-svelte": "^3.1.0",
    "jquery": "^3.6.0",
    "material-components-web": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss": "^8.4.6",
    "qs": "^6.10.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "sass": "^1.32.12",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^4.10.3",
    "svelte-scrollto": "^0.2.0"
  }
}
